        <script>
            document.addEventListener("DOMContentLoaded", function () {
                const descriptionLink = document.getElementById("descriptionLink");
                const reviewsLink = document.getElementById("reviewsLink");
                const detailsLink = document.getElementById("detailsLink");
                const activeLine = document.getElementById("activeLine");

                const descriptionContent = document.getElementById("descriptionContent");
                const reviewsContent = document.getElementById("reviewsContent");
                const detailsContent = document.getElementById("detailsContent");

                // Show description by default
                descriptionContent.style.display = "block";
                reviewsContent.style.display = "none";
                detailsContent.style.display = "none";

                // Set initial position of the active line
                activeLine.style.transform = "translateX(0)";

                descriptionLink.addEventListener("click", function () {
                    if (!descriptionLink.classList.contains("active-link")) {
                        descriptionContent.style.display = "block";
                        reviewsContent.style.display = "none";
                        detailsContent.style.display = "none";

                        // Move active line to description
                        activeLine.style.transform = "translateX(0)";

                        // Update link colors
                        descriptionLink.classList.add("active-link");
                        reviewsLink.classList.remove("active-link");
                        detailsLink.classList.remove("active-link");

                        // Make description link color black
                        descriptionLink.classList.remove("inactive-link");
                        reviewsLink.classList.add("inactive-link");
                        detailsLink.classList.add("inactive-link");
                    }
                });

                reviewsLink.addEventListener("click", function () {
                    if (!reviewsLink.classList.contains("active-link")) {
                        descriptionContent.style.display = "none";
                        reviewsContent.style.display = "block";
                        detailsContent.style.display = "none";

                        // Move active line to reviews
                        activeLine.style.transform = "translateX(100%)";

                        // Update link colors
                        descriptionLink.classList.remove("active-link");
                        reviewsLink.classList.add("active-link");
                        detailsLink.classList.remove("active-link");

                        // Make reviews link color black
                        descriptionLink.classList.add("inactive-link");
                        reviewsLink.classList.remove("inactive-link");
                        detailsLink.classList.add("inactive-link");
                    }
                });

                detailsLink.addEventListener("click", function () {
                    if (!detailsLink.classList.contains("active-link")) {
                        descriptionContent.style.display = "none";
                        reviewsContent.style.display = "none";
                        detailsContent.style.display = "block";

                        // Move active line to details
                        activeLine.style.transform = "translateX(200%)";

                        // Update link colors
                        descriptionLink.classList.remove("active-link");
                        reviewsLink.classList.remove("active-link");
                        detailsLink.classList.add("active-link");

                        // Make details link color black
                        descriptionLink.classList.add("inactive-link");
                        reviewsLink.classList.add("inactive-link");
                        detailsLink.classList.remove("inactive-link");
                    }
                });
            });
