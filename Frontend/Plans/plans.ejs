    <script>
        // Show Heart content by default
        document.getElementById('javaContent').style.display = 'block';

        // Function to toggle visibility of dropdown options
        function toggleDropdown() {
            var dropdownOptions = document.getElementById('dropdownOptions');
            dropdownOptions.classList.toggle('hidden');
        }

        // Function to show Heart content and hide Brain content
        function showJava() {
            document.getElementById('javaContent').style.display = 'block';
            document.getElementById('pythonContent').style.display = 'none';
            document.getElementById('selectedLanguage').textContent = 'Heart';
            toggleDropdown(); // Hide dropdown after selection
        }

        // Function to show Brain content and hide Heart content
        function showPython() {
            document.getElementById('javaContent').style.display = 'none';
            document.getElementById('pythonContent').style.display = 'block';
            document.getElementById('selectedLanguage').textContent = 'Brain';
            toggleDropdown(); // Hide dropdown after selection
        }

        // Close dropdown when clicking outside of it
        window.addEventListener('click', function (event) {
            var dropdownBtn = document.getElementById('dropdownBtn');
            var dropdownOptions = document.getElementById('dropdownOptions');
            if (!dropdownBtn.contains(event.target) && !dropdownOptions.contains(event.target)) {
                dropdownOptions.classList.add('hidden');
            }
        });
    </script>



































